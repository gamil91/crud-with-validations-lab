<h2> All Songs </h2>
<table style="width:50%">
    <thead style="text-align:center">
    <tr>
        <th>Title</th>
        <th>Release Year</th>
        <th>Released</th>
        <th>Genre</th>
        <th>Artist Name</th>
        <th>Edit</th>
        <th>Delete</th>
    </tr>
    </thead>
    <tbody>
        <tr style="text-align:center">
        <% @songs.each do |song|%>
        <td><%=link_to song.title, song_path(song) %></td>
        <td><%= song.release_year %></td>
        <td><%= song.released %></td>
        <td><%= song.genre %></td>
        <td><%= song.artist_name %></td>
        <td><%=link_to "Edit", "/songs/#{song.id}/edit", method: :patch, data:{confirm: "are you sure?"}%></td>
        <td><%=link_to "Delete", song, method: :delete, data:{confirm: "are you sure?"}%></td>
        <% end %>
    <tr>
    </tbody>
</table>
<br/>
<%=link_to "Create New Song", new_song_path%>






